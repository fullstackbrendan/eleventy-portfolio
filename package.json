{
	"name": "eleventy-portfolio",
	"version": "1.0.1",
	"description": "A Portfolio site using the Eleventy site generator.",
	"type": "module",
	"scripts": {
		"build": "npx @11ty/eleventy",
		"build-ghpages": "npx @11ty/eleventy --pathprefix=/eleventy-base-blog/",
		"start": "npx @11ty/eleventy --serve --quiet",
		"start-ghpages": "npx @11ty/eleventy --pathprefix=/eleventy-base-blog/ --serve --quiet",
		"debug": "DEBUG=Eleventy* npx @11ty/eleventy",
		"debugstart": "DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
		"benchmark": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy"
	},
	"author": {
		"name": "Brendan Enang",
		"email": "bnn.cdla@gmail.com",
		"url": "https://fullstackbrendan.dev/"
	},
	"license": "MIT",
	"engines": {
		"node": ">=18"
	},
	"devDependencies": {
		"@11ty/eleventy": "3.0.0-alpha.13",
		"@11ty/eleventy-img": "5.0.0-beta.4",
		"@11ty/eleventy-navigation": "^0.3.5",
		"@11ty/eleventy-plugin-rss": "2.0.0-beta.8",
		"@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
		"luxon": "^3.4.4",
		"markdown-it-anchor": "^8.6.7",
		"zod": "^3.23.8",
		"zod-validation-error": "^3.3.0"
	},
	"dependencies": {
		"@11ty/eleventy-cache-assets": "^2.3.0",
		"@11ty/eleventy-fetch": "^4.0.1",
		"axios": "^1.7.2",
		"base-64": "^1.0.0",
		"dotenv": "^16.4.5",
		"eleventy-plugin-embed-everything": "^1.18.2",
		"eleventy-plugin-embed-soundcloud": "^1.2.7",
		"eleventy-plugin-embed-spotify": "^1.3.0"
	}
}